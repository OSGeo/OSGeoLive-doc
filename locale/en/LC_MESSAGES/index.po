msgstr ""

#: ../../build/doc/index.rst:25
msgid "Press \"Enter\" to startup & login."
msgstr ""

#: ../../build/doc/index.rst:26
msgid "Select and run applications from the \"Geospatial\" menu."
msgstr ""

